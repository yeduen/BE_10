     "output_type": "stream",
     "text": [
      " 지훈의 길이 : 2\n",
      " 예든의 길이 : 2\n",
      " 이든의 길이 : 2\n"
     ]
    }
   ],
   "source": [
    "def print_lengths(strings):\n",
    "    for strings in strings:\n",
    "        print(f' {strings}의 길이 : {len(strings)}')\n",
    "sample_strings = [\"지훈\", \"예든\", \"이든\"]\n",
    "print_lengths(sample_strings)"
   ]
